    <body>
	
		<div class="container">
			<div class="row">
				<div class="col-md-7 col-md-offset-1 site">
				
						<div class="site-title">
							<h2><b>NodeLog.in</b></h2>
						</div><! --/site top -->
			

							{{#each entries}}
							<div class="post">
								<div class="row">
              						<div class="col-md-2 contrib">
              						<img class="contrib-pic" src="static/img/pic-mockup.jpeg"></img>
              						<small>{{this.contrib}}</small>
              						</div>
              						<div class="col-md-10">
									<h4>{{this.title}}</h4>
									<small>{{this.site}}</small>
									<p class="pull-right"><a href="{{this.link}}" <span class="glyphicon glyphicon-new-window"></span></span></a></p>
									</div>
						   		 </div>
							</div>
						    {{/each}}


							<div class="post centered">
								newletter
							</div>
							
							<div class="site-footer">
								<p><a href="#">Home</a> | <a href="#">Contact</a> | <a href="#">Submit</a>  </p>
							</div>
					
				</div><!-- /col-md-6 -->
			</div><! --/row -->
		</div><! --/container -->
		        

        

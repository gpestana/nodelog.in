    <meta name="description" content="daily bundle of interesting nodeJS content. created and curated by the community">
    <meta name="author" content="gpestana">
    <link rel="shortcut icon" href="assets/ico/favicon.ico">

    <title>NodeLog.in</title>

    <link href="static/css/bootstrap.css" rel="stylesheet">
